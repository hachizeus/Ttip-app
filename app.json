{
  "expo": {
    "name": "TTip",
    "slug": "ttip-digital-tipping",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/mylogo.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/mylogo.png",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ttip.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses camera to scan QR codes for tipping",
        "NSFaceIDUsageDescription": "Use Face ID to securely authenticate and access your account"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/mylogo.png"
      },
      "package": "com.ttip.app",
      "versionCode": 1,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.USE_FINGERPRINT",
        "android.permission.USE_BIOMETRIC",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/mylogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Use Face ID to authenticate yourself."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "scheme": "ttip",
    "privacy": "public",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ee134ee9-2703-44c2-8c92-f1a65e3d8b1f"
      }
    },
    "owner": "elitjohns"
  }
}
